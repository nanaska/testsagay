
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Product {
    id Int @id @default(autoincrement())
    title String
    description String
    type Int
    price Int
    img String
    count Int @default(0)
    pizzaFilterType Int
    rollsFilterType Int
}
